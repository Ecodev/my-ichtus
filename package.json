{
    "name": "my-ichtus",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "client-config": "`which php8.1 || which php` ./bin/generate-client-configuration.php",
        "postinstall": "playwright install chromium",
        "prerequisite": "mkdir -p htdocs/ && cp -v server/index.php htdocs/ && yarn client-config && yarn codegen",
        "dev": "yarn prerequisite && ng serve",
        "prod": "yarn prerequisite && ng build && ng build vanilla && find data/tmp/build -type f -not -name '*.gz' -exec gzip --keep --best {} \\; && find htdocs/ -mindepth 1 -not -path 'htdocs/data*' -delete && cp -rv data/tmp/build/* htdocs/ && mv -v data/tmp/build-vanilla htdocs/vanilla",
        "test": "yarn prerequisite && ng test",
        "lint": "ng lint",
        "codegen": "./bin/dump-schema && apollo client:codegen -c apollo.config.js --passthroughCustomScalars --customScalarsPrefix=App --outputFlat --target typescript client/app/shared/generated-types.ts"
    },
    "dependencies": {
        "@angular/animations": "^13.2.4",
        "@angular/cdk": "~13.2.4",
        "@angular/common": "~13.2.4",
        "@angular/compiler": "~13.2.4",
        "@angular/core": "~13.2.4",
        "@angular/flex-layout": "^13.0.0-beta.38",
        "@angular/forms": "^13.2.4",
        "@angular/localize": "^13.2.4",
        "@angular/material": "^13.2.4",
        "@angular/platform-browser": "~13.2.4",
        "@angular/platform-browser-dynamic": "~13.2.4",
        "@angular/router": "~13.2.4",
        "@apollo/client": "^3.5.10",
        "@ecodev/fab-speed-dial": "^10.0.0",
        "@ecodev/natural": "^42.4.1",
        "@ecodev/natural-editor": "^42.4.1",
        "@graphql-tools/mock": "^8.5.2",
        "angular-particle-effect-button": "^0.0.50",
        "apollo": "^2.33.9",
        "apollo-angular": "^3.0.0",
        "apollo-upload-client": "^17.0.0",
        "big.js": "^6.1.1",
        "crypto-es": "^1.2.7",
        "graphql": "^15.6.1",
        "ibantools": "^4.1.4",
        "jsqr": "^1.4.0",
        "lodash-es": "^4.17.21",
        "ngx-progressbar": "^8.0.0",
        "ngx-timeago": "^2.0.0",
        "rxjs": "~7.5.4",
        "tslib": "^2.3.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.2.5",
        "@angular-eslint/builder": "13.1.0",
        "@angular-eslint/eslint-plugin": "13.1.0",
        "@angular-eslint/eslint-plugin-template": "13.1.0",
        "@angular-eslint/schematics": "14.0.0",
        "@angular-eslint/template-parser": "13.1.0",
        "@angular/cli": "~14.0.4",
        "@angular/compiler-cli": "~13.2.4",
        "@playwright/test": "^1.19.2",
        "@types/apollo-upload-client": "^17.0.0",
        "@types/big.js": "^6.1.3",
        "@types/jasmine": "~3.10.3",
        "@types/jasminewd2": "~2.0.10",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^14.14.31",
        "@typescript-eslint/eslint-plugin": "5.13.0",
        "@typescript-eslint/parser": "5.13.0",
        "eslint": "^8.10.0",
        "jasmine-core": "~4.0.1",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "~6.3.17",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.7.0",
        "prettier": "2.5.1",
        "ts-node": "~10.6.0",
        "typescript": "~4.5.5"
    }
}
