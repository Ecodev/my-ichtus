{
    "name": "my-ichtus",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "client-config": "`which php7.4 || which php` ./bin/generate-client-configuration.php",
        "postinstall": "playwright install chromium",
        "prerequisite": "mkdir -p htdocs/ && cp -v server/index.php htdocs/ && yarn client-config && yarn codegen",
        "dev": "yarn prerequisite && ng serve",
        "prod": "yarn prerequisite && ng build && ng build vanilla && find data/tmp/build -type f -not -name '*.gz' -exec gzip --keep --best {} \\; && find htdocs/ -mindepth 1 -not -path 'htdocs/data*' -delete && cp -rv data/tmp/build/* htdocs/ && mv -v data/tmp/build-vanilla htdocs/vanilla",
        "test": "yarn prerequisite && ng test",
        "lint": "ng lint",
        "codegen": "./bin/dump-schema && apollo client:codegen -c apollo.config.js --passthroughCustomScalars --customScalarsPrefix=App --outputFlat --target typescript client/app/shared/generated-types.ts"
    },
    "dependencies": {
        "@angular/animations": "~12.2.9",
        "@angular/cdk": "~12.2.9",
        "@angular/common": "~12.2.9",
        "@angular/compiler": "~12.2.9",
        "@angular/core": "~12.2.9",
        "@angular/flex-layout": "^12.0.0-beta.35",
        "@angular/forms": "~12.2.9",
        "@angular/localize": "^12.2.9",
        "@angular/material": "^12.2.9",
        "@angular/platform-browser": "~12.2.9",
        "@angular/platform-browser-dynamic": "~12.2.9",
        "@angular/router": "~12.2.9",
        "@apollo/client": "^3.4.16",
        "@ecodev/fab-speed-dial": "^9.0.0",
        "@ecodev/natural": "^38.1.0",
        "@graphql-tools/mock": "^8.4.0",
        "angular-particle-effect-button": "^0.0.50",
        "apollo": "^2.33.6",
        "apollo-angular": "^2.6.0",
        "apollo-upload-client": "^16.0.0",
        "crypto-es": "^1.2.7",
        "graphql": "^15.6.1",
        "ibantools": "^3.3.1",
        "jsqr": "^1.4.0",
        "lodash-es": "^4.17.21",
        "ngx-progressbar": "^6.1.1",
        "ngx-timeago": "^2.0.0",
        "prosemirror-example-setup": "^1.1.2",
        "prosemirror-keymap": "^1.1.4",
        "prosemirror-schema-basic": "^1.1.2",
        "prosemirror-state": "^1.3.4",
        "prosemirror-view": "^1.20.2",
        "rxjs": "~7.4.0",
        "tslib": "^2.3.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.2.9",
        "@angular/cli": "~13.0.3",
        "@angular/compiler-cli": "~12.2.9",
        "@playwright/test": "^1.15.2",
        "@types/apollo-upload-client": "^14.1.0",
        "@types/jasmine": "~3.9.1",
        "@types/jasminewd2": "~2.0.10",
        "@types/lodash-es": "^4.17.5",
        "@types/node": "^14.14.31",
        "@types/prosemirror-model": "^1.13.2",
        "@types/prosemirror-schema-basic": "^1.0.2",
        "@types/prosemirror-schema-list": "^1.0.3",
        "@types/prosemirror-state": "^1.2.7",
        "@types/prosemirror-view": "^1.19.1",
        "codelyzer": "^6.0.2",
        "jasmine-core": "~3.9.0",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "~6.3.4",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.7.0",
        "prettier": "2.4.1",
        "ts-node": "~10.2.1",
        "tslint": "~6.1.3",
        "typescript": "~4.3.5"
    }
}
