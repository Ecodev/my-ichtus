{
    "name": "my-ichtus",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "prerequisite": "mkdir -p htdocs/ && cp -v server/index.php htdocs/ && yarn codegen",
        "dev": "yarn prerequisite && ng serve",
        "prod": "yarn prerequisite && ng build --prod && ng build --prod vanilla && find htdocs/ -mindepth 1 -not -path 'htdocs/data*' -delete && cp -rv data/tmp/build/* htdocs/ && mv -v data/tmp/build-vanilla htdocs/vanilla",
        "test": "yarn prerequisite && ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "codegen": "./bin/dump-schema && apollo client:codegen -c apollo.config.js  --outputFlat --target typescript client/app/shared/generated-types.ts"
    },
    "browser": {
        "crypto": false
    },
    "dependencies": {
        "@angular/animations": "~11.2.0",
        "@angular/cdk": "~11.2.0",
        "@angular/common": "~11.2.0",
        "@angular/compiler": "~11.2.0",
        "@angular/core": "~11.2.0",
        "@angular/flex-layout": "^11.0.0-beta.33",
        "@angular/forms": "~11.2.0",
        "@angular/localize": "^11.2.0",
        "@angular/material": "^11.2.0",
        "@angular/platform-browser": "~11.2.0",
        "@angular/platform-browser-dynamic": "~11.2.0",
        "@angular/router": "~11.2.0",
        "@apollo/client": "^3.3.11",
        "@ecodev/fab-speed-dial": "^7.0.0",
        "@ecodev/natural": "^34.0.0",
        "@graphql-tools/mock": "^8.0.0",
        "angular-particle-effect-button": "^0.0.50",
        "apollo": "^2.32.1",
        "apollo-angular": "^2.3.0",
        "apollo-upload-client": "^14.1.3",
        "crypto-js": "^4.0.0",
        "graphql": "^15.5.0",
        "ibantools": "^3.2.3",
        "jsqr": "^1.3.1",
        "lodash-es": "^4.17.20",
        "ngx-progressbar": "^6.1.0",
        "ngx-timeago": "^2.0.0",
        "prosemirror-example-setup": "^1.1.2",
        "prosemirror-keymap": "^1.1.4",
        "prosemirror-schema-basic": "^1.1.2",
        "prosemirror-state": "^1.3.4",
        "prosemirror-view": "^1.17.6",
        "puppeteer": "^7.1.0",
        "rxjs": "~6.6.3",
        "tslib": "^2.1.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1102.0",
